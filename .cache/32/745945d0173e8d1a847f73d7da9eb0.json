enerated":{"line":138,"column":55}},{"source":"src/HomePage.vue","name":null,"original":{"line":123,"column":0},"generated":{"line":139,"column":0}},{"source":"src/HomePage.vue","name":null,"original":{"line":112,"column":0},"generated":{"line":139,"column":7}},{"source":"src/HomePage.vue","name":null,"original":{"line":127,"column":0},"generated":{"line":140,"column":0}},{"source":"src/HomePage.vue","name":null,"original":{"line":94,"column":0},"generated":{"line":141,"column":0}},{"source":"src/HomePage.vue","name":null,"original":{"line":83,"column":0},"generated":{"line":142,"column":0}}],"sources":{"src/HomePage.vue":"<template>\r\n  <div>\r\n    <div v-if=\"changeForm === true\" id=\"login\" class=\"container\">\r\n      <div class=\"alert alert-warning text-center my-4\"></div>\r\n\r\n      <div class=\"row justify-content-center\">\r\n        <div class=\"col-12 col-md-8 col-lg-8 col-xl-6\">\r\n          <div class=\"row\">\r\n            <div class=\"col text-center\">\r\n              <h2>Login</h2>\r\n            </div>\r\n          </div>\r\n          <div class=\"row align-items-center mt-4\">\r\n            <div class=\"col\">\r\n              <input v-model=\"formLogin.Email\" type=\"email\" class=\"form-control\" placeholder=\"Email\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"row align-items-center mt-4\">\r\n            <div class=\"col\">\r\n              <input\r\n               v-model=\"formLogin.Password\"\r\n                type=\"password\"\r\n                class=\"form-control\"\r\n                placeholder=\"Password\"\r\n              />\r\n            </div>\r\n          </div>\r\n          <div class=\"row justify-content-start\">\r\n            <div class=\"col\">\r\n              <button v-on:click.prevent=\"login\" class=\"btn btn-primary mt-4\" type=\"submit\">Login</button>\r\n              <button v-on:click.prevent=\"changeForm = false\" class=\"btn btn-primary mt-4\">Register</button>\r\n              <center><h3>OR</h3></center>\r\n              <center>\r\n                <div class=\"g-signin2\" data-onsuccess=\"onSignIn\"></div>\r\n              </center>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-else-if=\"changeForm === false\" id=\"register\" class=\"container\">\r\n      <div class=\"alert alert-warning text-center my-4\">{{message}}</div>\r\n      <div class=\"row justify-content-center\">\r\n        <div class=\"col-12 col-md-8 col-lg-8 col-xl-6\">\r\n          <div class=\"row\">\r\n            <div class=\"col text-center\">\r\n              <h2>Register</h2>\r\n            </div>\r\n          </div>\r\n          <div class=\"row align-items-center mt-4\">\r\n            <div class=\"col\">\r\n              <input v-model=\"formLogin.Email\" type=\"email\" class=\"form-control\" placeholder=\"Email\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"row align-items-center mt-4\">\r\n            <div class=\"col\">\r\n              <input\r\n              v-model=\"formLogin.Password\"\r\n                type=\"password\"\r\n                class=\"form-control\"\r\n                placeholder=\"Password\"\r\n              />\r\n            </div>\r\n          </div>\r\n          <div class=\"row justify-content-start\">\r\n            <div class=\"col\">\r\n              <button \r\n             @click=\"formAdd\" class=\"btn btn-primary mt-4\">Submit</button>\r\n              <button v-on:click.prevent=\"changeForm = true\" class=\"btn btn-primary mt-4\">Back</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Axios from 'axios';\r\nAxios.defaults.baseURL = \"http://localhost:3000\";\r\n\r\nexport default {\r\n  data() {\r\n      return{\r\n        formLogin:{\r\n            Email:'',\r\n            Password:''\r\n        },\r\n        message:'',\r\n        changeForm: true\r\n     }\r\n  },\r\n  methods:{\r\n      formAdd(){\r\n        Axios({\r\n          method: \"POST\",\r\n          url: \"/register\",\r\n          headers: { \"Content-Type\": \"application/json\" },\r\n          data:this.formLogin\r\n        })\r\n        .then(({data})=>{\r\n          console.log(data),\r\n          this.message = data.message\r\n        })\r\n        .catch((err)=>[\r\n          console.log(err),\r\n          this.message = err\r\n        ])\r\n      },\r\n      login(){\r\n         Axios({\r\n          method: \"POST\",\r\n          url: \"/login\",\r\n          headers: { \"Content-Type\": \"application/json\" },\r\n          data:this.formLogin\r\n        })\r\n        .then(({data})=>{\r\n          localStorage.setItem(\"access_token\",data.access_token)\r\n          this.$emit(\"changePage\",true)\r\n          this.message = data.message\r\n        })\r\n        .catch((err)=>[\r\n          console.log(err),\r\n          this.message = err\r\n        ])\r\n      }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n</style>"},"lineCount":null}},"error":null,"hash":"46cffa7c53e2ece9d7bd08bbd718204f","cacheData":{"env":{}}}